    "#as we can imagine there will be factors which influence the sruvival factor\n",
    "#we will create a pivot table to count the number of survived people on the basis of sex \n",
    "\n",
    "\n",
    "Sex_sur_Df = pd.pivot_table(df_train,\n",
    "                            index='Sex',\n",
    "                            columns='Survived',\n",
    "                            values='PassengerId',\n",
    "                            aggfunc='count')\n",
    "Sex_sur_Df"
   ]
