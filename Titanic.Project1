   "id": "54fa4261",
   "metadata": {},
   "outputs": [],
   "source": [
    "#we can now translate the embarked and sex columns with boolean values since it will be easier to use the model\n",
    "embark=[]\n",
    "for i in df[\"Embarked\"]:\n",
    "    if i==\"S\":\n",
    "        embark.append(0)\n",
    "    elif i==\"Q\":\n",
    "        embark.append(1)\n",
    "    elif i==\"C\":\n",
    "        embark.append(2)\n",
    "    else:\n",
    "        embark.append(3)\n",
    "sex=[]\n",
    "for i in df[\"Sex\"]:\n",
    "    if i=='M':\n",
    "        sex.append(0)\n",
    "    else:\n",
    "        sex.append(1)\n",
    "        \n",
    "df=df.drop([\"Sex\"], axis=1)\n",
    "df=df.drop([\"Embarked\"], axis=1)\n",
    "\n",
    "df[\"Sex\"]=sex\n",
    "df[\"Embarked\"]=embark"
   ]
  },
